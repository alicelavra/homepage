	if (starhit.article.id) {
		let comments = document.querySelector('div.comments');
		if (comments) {
			comments.dataset.scrollEvent = 'comments_v2';
		}

		let recommend = document.querySelector('article.article-related');
		if (recommend) {
			recommend.dataset.scrollEvent = 'recommend';
		}

		let relap = document.querySelector('div.relap-wrap-wrap');
		if (relap) {
			relap.dataset.scrollEvent = 'relap';
		}

		let latest = document.querySelector('div.cats-wrap');
		if (latest) {
			latest.dataset.scrollEvent = 'latest';
		}

		let teasers = document.querySelector('div.teaser_slot');
		if (teasers) {
			teasers.dataset.scrollEvent = 'teasers';
		}

		let scrollObserver = new function () {
			this.threshold = [];
			for (let i = 0; i <= 1; i = i + 0.1) {
				this.threshold.push(i);
			}

			let article_container = document.querySelector('article.article');
			let scroll_event_items = document.querySelectorAll('[data-scroll-event]');
			let scroll_stops = [0, 25, 50, 75, 100];
			let time = (new Date().getTime()) / 1000;

			let default_params = {scroll: {}};
			default_params.scroll['article_' + starhit.article.id] = {};

			let scrollWatch = function () {
				let box = article_container.getBoundingClientRect();
				let vh = document.documentElement.clientHeight;

				let to_bottom = box.bottom - vh;
				let part = 100 - Math.round(to_bottom / box.height * 100);

				if (part > 100) {
					part = 100;
				}

				scroll_stops.forEach(function (stop, i, stops) {
					if (part >= stop) {
						let now = (new Date().getTime()) / 1000;
						let time_gap = Math.round((now - time) * 10) / 10;
						let params = Object.assign({}, default_params);
						stop = ('00' + stop).slice(-3);

						params['scroll']['article_' + starhit.article.id]['body_' + stop] = time_gap;

						starhit.metrika.sendParams(params);

						if (stop > 0) {
							let params = stop >= 50 ? {nonInteraction: true} : {};

							ga('send', 'event', 'SCROLL', 'ARTICLE_' + stop, starhit.device, params);
						}

						//console.log('article is scrolled by ' + stop + '%. Time gap is ' + time_gap + ' seconds');

						stops.splice(i, 1);
					}
				});

				if (scroll_stops.length == 0) {
					window.removeEventListener('scroll', scrollWatch)
					observer.unobserve(article_container);
				}
			};

			let observer = new IntersectionObserver(function (entries) {
				for (let entry of entries) {
					let element = entry.target;

					if (element == article_container) {
						if (entry.isIntersecting) {
							window.addEventListener('scroll', scrollWatch)
						} else {
							window.removeEventListener('scroll', scrollWatch)
						}
					} else if (entry.isIntersecting && 'scrollEvent' in element.dataset) {
						let event = element.dataset.scrollEvent;
						delete element.dataset.scrollEvent;
						let now = (new Date().getTime()) / 1000;
						let time_gap = Math.round((now - time) * 10) / 10;
						let params = Object.assign({}, default_params);

						params['scroll']['article_' + starhit.article.id][event] = time_gap;

						starhit.metrika.sendParams(params);

						if (event == 'relap' && 'showNewsFeed' in starhit && document.querySelector('.news-feed')) {
							let news_feed = document.querySelector('.news-feed');
							let container = document.querySelector('.wrap > .container');
							let shadow_width = 15;

							let show_lenta = window.innerWidth - container.offsetWidth > news_feed.offsetWidth * 2 + shadow_width;

							if (show_lenta) {
								starhit.metrika.sendGoal('lenta_btn_click');
								starhit.showNewsFeed();
							}
						}

						//console.log('trigger scroll event ' + event);
						observer.unobserve(element);
					}
				}
			}, {rootMargin: '0px 0px -20px 0px', threshold: this.threshold});

			observer.observe(article_container);
			scroll_event_items.forEach(function (item) {
				observer.observe(item);
			});

			return observer;
		}

		if (typeof PerformanceObserver !== 'undefined') {
			new PerformanceObserver(entryList => {
				let entries = entryList.getEntries();
				let entry = entries[entries.length - 1];

				let lcp = Math.ceil((entry.renderTime || entry.loadTime) / 100) / 10; //милисекунды в секунды с округлением

				let art_key = 'article_' + starhit.article.id;

				let params = {};
				params['cwv'] = {};
				params['cwv']['lcp'] = {};
				params['cwv']['lcp'][starhit.category] = {};
				params['cwv']['lcp'][starhit.category][art_key] = lcp;

				if (lcp >= 2.5) {
					let element;
					if (entry.element) {
						element = starhit.elementToString(entry.element);
					} else if (entry.url) {
						element = entry.url;
					}

					params['bad_lcp'] = {};
					params['bad_lcp'][starhit.category] = {};
					params['bad_lcp'][starhit.category][art_key] = {}
					params['bad_lcp'][starhit.category][art_key][element] = lcp;
				}

				starhit.metrika.sendParams(params);
			}).observe({type: "largest-contentful-paint", buffered: true});
		}

	}
