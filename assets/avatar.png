<script>

	starhit.hbmid = false;

	starhit.inread = new function() {
		this.div = null;

		this.getDiv = function () {
			if (!this.div) {
				this.div = this.makeDiv();
			}

			return this.div;
		};

		this.makeDiv = function () {
			let div_inread = document.createElement('div');
			div_inread.setAttribute('id', 'div-inread');

			return div_inread;
		};
		
		this.placeCheck = function (i_element,article_elements) {
			let _i_element = i_element-1;
			for(let ii=_i_element; ii<=article_elements.length; ii++) {
				if (ii in article_elements && article_elements[ii].matches('h2,script,.viqeo-embed')) {
					i_element++;
				} else {
					break;
				}
			}
			return i_element;
		};

		this.placeInArticle = function () {
			let article_elements = document.querySelector('article.article').children;
			let current_height = 0;

			let item_element = '';
			
			let sl = divisor = 2;
			let limit_height = 5000;
			let div = limit_height/divisor;
			for(let i_div=3; i_div>=1; i_div--) {
				if(starhit.article.height>limit_height*i_div) {
					divisor = Math.round(starhit.article.height/div)*i_div;
					divisor = divisor - i_div;
					if(divisor <= sl) {
						divisor = divisor + sl;
					}
					break;
				}
			}
			
			for (let i in article_elements) {
				if (!article_elements.hasOwnProperty(i)) {
					continue;
				}
				
				if (i>0 && i-1 in article_elements && article_elements[i-1].matches('h2,script,.viqeo-embed')) {
					continue;
				}

				let element = article_elements[i];

				current_height += element.offsetHeight;

				if (current_height >= starhit.article.height / divisor) {
					element.parentNode.insertBefore(this.getDiv(), element);
					this.load();
					item_element = i;
					break;
				}
			}
			
			if(item_element) {
				let i_element = Number(item_element)+5;

				i_element = this.placeCheck(i_element,article_elements);

				if(article_elements.hasOwnProperty(i_element)) {
					let div_hb_mid = document.createElement('div');
					div_hb_mid.setAttribute('id', 'div-hb-mid');
					let _element = article_elements[i_element];
					_element.parentNode.insertBefore(div_hb_mid, _element);
					starhit.hbmid = true;
					dfp.defineSlot('hb-mid');
					dfp.refresh('inread');
				}
				
				for (let i_inline=1; i_inline<=10; i_inline++) {
					i_element = i_element+7;
					i_element = this.placeCheck(i_element,article_elements);
					if(article_elements.hasOwnProperty(i_element)) {
						let div_hb_inline = document.createElement('div');
						div_hb_inline.setAttribute('id', 'div-hb-inline-'+i_inline);
